class PalindromeChecker {
 
    boolean isPalindrome(int number) {
        int original = number;
        int reverse = 0;

       
        while (number > 0) {
            int digit = number % 10;
            reverse = reverse * 10 + digit;
            number /= 10;
        }

      
        return original == reverse;
    }
}

public class Main {
    public static void main(String[] args) {
        PalindromeChecker checker = new PalindromeChecker();

        int[] rollNumbers = {1221, 1234, 454};

        for (int roll : rollNumbers) {
            if (checker.isPalindrome(roll)) {
                System.out.println("Roll " + roll + " → Palindrome");
            } else {
                System.out.println("Roll " + roll + " → Not Palindrome");
            }
        }
    }
}









