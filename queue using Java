import java.util.LinkedList;
import java.util.Queue;

public class Main {
    public static void main(String[] args) {

      
        Queue<String> queue = new LinkedList<>();

     
        queue.add("Alice");
        queue.add("Bob");
        queue.add("Charlie");
        queue.add("David");
        queue.add("Emma");

      
        System.out.println("Queue initially: " + queue);

        
        while (!queue.isEmpty()) {
            String customer = queue.poll(); // Removes and returns the head of the queue
            System.out.println("Serving " + customer);
        }

       
        System.out.println("Final queue empty â†’ " + queue.isEmpty());
    }
}
