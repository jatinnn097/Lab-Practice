package fib;

public class FibonacciGenerator {
	 void generate(int n) {
		 
		 int a=0 ,b=1;
		 System.out.println("Fibonacci Series:");        
		 for (int i=1; i <= n; i++) {
	            System.out.print(a + " ");
	            int next = a + b;
	            a = b;
	            b = next;
	        }
	        System.out.println();
	 }
}
package fib;
public class Main {
    public static void main(String[] args) {
        FibonacciGenerator fg = new FibonacciGenerator();

        int n = 7; // Example: You can change this value for testing
        fg.generate(n);
    }
}

